# Xinference设置
XINFERENCE_API_BASE=https://api.deepseek.com/v1
XINFERENCE_API_KEY=your_actual_api_key

# 模型设置
MODEL_NAME=deepseek-chat
TEMPERATURE=0.6
MAX_TOKENS=8192

# Manim设置
MANIM_OUTPUT_DIR=./media
MANIM_QUALITY=low_quality  # low_quality(快速), medium_quality(平衡), high_quality(高质量但慢)

# 性能优化设置（速度 vs 质量权衡）
ENABLE_UNDERSTANDING_AGENT=true  # true=详细分析题目, false=跳过分析直接解题(极速模式)
ENABLE_REVIEW_AGENT=false  # true=启用代码审查(质量更好但慢), false=禁用(速度快)
MAX_DEBUG_ATTEMPTS=2  # 最大调试次数(1-3), 减少可提速但可能失败率高

# 性能模式预设：
# 极速模式: ENABLE_UNDERSTANDING_AGENT=false, ENABLE_REVIEW_AGENT=false, MAX_DEBUG_ATTEMPTS=1, MANIM_QUALITY=low_quality
# 平衡模式: ENABLE_UNDERSTANDING_AGENT=true, ENABLE_REVIEW_AGENT=false, MAX_DEBUG_ATTEMPTS=2, MANIM_QUALITY=low_quality
# 高质量模式: ENABLE_UNDERSTANDING_AGENT=true, ENABLE_REVIEW_AGENT=true, MAX_DEBUG_ATTEMPTS=3, MANIM_QUALITY=medium_quality
